{
  "name": "radio-quoting-tool",
  "version": "1.0.0",
  "description": "AI-powered quoting tool for two-way radio and surveillance systems",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && node server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "cd server && node server.js",
    "init-db": "cd server && node initDatabase.js",
    "init-enhanced": "cd server && node database/enhanced-init.js"
  },
  "keywords": ["radio", "quoting", "ai", "motorola", "surveillance"],
  "author": "Radio Quoting System",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "pdfkit": "^0.14.0",
    "moment": "^2.29.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}